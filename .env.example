# 网站标题
VITE_APP_TITLE = Mini-Cover

# 网站的ICP备案号码，footer 中展示备案信息
VITE_APP_ICP_NUMBER = 

# ==================== 兰空图床配置 ====================
# 兰空图床上传API地址
# 格式：https://your-domain.com/api/v2/upload (注意是v2版本)
VITE_APP_UPLOAD_API_URL = https://your-lsky-domain.com/api/v2/upload

# 兰空图床Token配置（必需）
# 获取方式：登录兰空图床后台 -> 个人设置 -> API Token
# 格式示例：Bearer your_token_here 或直接 your_token_here
VITE_APP_UPLOAD_API_TOKEN = Bearer your_token_here

# 存储策略ID（可选）
# 留空：在设置面板中选择
# 指定ID：直接使用指定的存储策略
VITE_APP_LSKY_STORAGE_ID = 

# ==================== 字体配置 ====================
# 设置全局字体 （可选） 不填写则默认
VITE_APP_FONT_FAMILY = LXGW Bright Medium

# 引入网络字体链接 （可选） 不填写则不引入
VITE_APP_FONT_CSS_URL = https://oss-cdn.bsgun.cn/fonts/LXGW-Bright-Medium/font.css